{
  "name": "kg-backend2",
  "version": "0.0.0",
  "scripts": {
    "prepareDeploy": "tsn ./scripts/prepareDeploy.ts",
    "deploy": "build-prod && yarn prepareDeploy && gcloud app deploy ./build/deploy/app.yaml --project kg-backend2 --version 1 --quiet",
    "deploy-index": "gcloud app deploy ./index.yaml --project kg-backend2",
    "serve": "APP_ENV=dev nodemon -e ts,js,json --exec ts-node -- -r tsconfig-paths/register ./src/startServer.ts",
    "startJs": "TS_NODE_PROJECT=./tsconfig.dist.json APP_ENV=dev yarn start",
    "start": "node -r tsconfig-paths/register ./dist/startServer.js",
    "test-int": "TEST_MODE=integration yarn test"
  },
  "dependencies": {
    "@google-cloud/datastore": "^3.0.1",
    "@google-cloud/logging-winston": "^0.10.2",
    "@naturalcycles/backend-lib": "^1.0.0",
    "@naturalcycles/js-lib": "^2.0.2",
    "@naturalcycles/nodejs-lib": "^3.0.0",
    "@naturalcycles/promise-lib": "^2.0.0",
    "@sentry/node": "^4.5.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-promise-router": "^3.0.3",
    "firebase-admin": "^7.0.0",
    "fs-extra": "^7.0.1",
    "got": "^9.6.0",
    "helmet": "^3.15.0",
    "joi": "^14.3.1",
    "lru-cache": "^5.1.1",
    "luxon": "^1.10.0",
    "nanoid": "^2.0.1",
    "rxjs": "^6.3.3",
    "slugify": "^1.3.4",
    "swagger-stats": "^0.95.7",
    "tsconfig-paths": "^3.7.0",
    "winston": "^3.1.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@naturalcycles/shared-module": "^2.1.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/google-cloud__datastore": "^1.3.3",
    "@types/helmet": "^0.0.42",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "jest": "^23.6.0",
    "jest-junit": "^6.0.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.0",
    "supertest": "^3.4.2",
    "timemachine": "^0.3.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  },
  "engines": {
    "node": "^10.0.0"
  },
  "description": "",
  "author": "Kirill Groshkov",
  "license": "MIT"
}
